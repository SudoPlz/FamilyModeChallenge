{
  "name": "FamilyModeChallenge",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "setup": "bundle install && yarn podInstall && rm -rf ios/.xcode.env.local",
    "startClean": "yarn start -- --reset-cache",
    "podInstall": "cd ios && bundle exec pod install && cd ..",
    "cleanWatchman": "watchman watch-del-all",
    "deletePods": "rm -rf ios/Pods",
    "cleanInstall": "yarn cleanWatchman && yarn install --immutable && bundle install && yarn deletePods && yarn podInstall",
    "nuclear": "yarn cleanInstall && yarn startClean",
    "test": "jest",
    "reverse": "adb -s emulator-5554 reverse tcp:4001 tcp:4001",
    "adb-log": "adb logcat *:E",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/material-top-tabs": "^6.6.13",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.2",
    "axios": "^1.6.8",
    "formik": "^2.4.5",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-circular-progress-indicator": "^4.4.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-keychain": "^8.2.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^2.12.2",
    "react-native-pager-view": "^6.3.1",
    "react-native-particles": "^0.0.8",
    "react-native-reanimated": "^3.12.0",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.25.0",
    "react-native-svg": "^15.3.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-vector-icons": "^10.0.3",
    "react-native-wagmi-charts": "^2.5.1",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "^7.6.3",
    "@react-native-community/slider": "^4.5.0",
    "@react-native/babel-preset": "0.74.84",
    "@react-native/eslint-config": "0.74.84",
    "@react-native/metro-config": "0.74.84",
    "@react-native/typescript-config": "0.74.84",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.4.4",
    "@types/luxon": "^3.4.2",
    "@types/react": "^18.2.6",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-vector-icons": "^6.4.14",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.6.3",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "detox": "^20.20.3",
    "eslint": "^8.19.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-jest": "^28.5.0",
    "husky": "^8.0.0",
    "jest": "^29.6.3",
    "lint-staged": ">=15.2.2",
    "nock": "^13.5.4",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "tsc-files": "^1.1.4",
    "typescript": "5.0.4",
    "yarn-audit-fix": "^10.0.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ],
    "*.{ts,tsx}": [
      "tsc-files"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
